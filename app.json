{
  "expo": {
    "name": "Hinode",
    "slug": "Hinode",
    "version": "1.0.0",
    "assetBundlePatterns": [
      "**/*"
    ],
    "orientation" : "portrait",
    "plugins": [
      [
        "expo-media-library",{
          "photosPermission" : "Permissões para fotos",
          "savePhotosPermission" : "Permissões para salvar as fotos",
          "isAccessMediaLocationEnabled": true
        }
      ]
    ],
    "android": {
			"package": "ao.hinode.kurogai",
			"permissions": [
				"android.permission.CAMERA",
				"android.permission.INTERNET",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
			]
		}
  }
}
